@model IEnumerable<JVStoreFront.DATA.EF.StockStatus>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StockStatusName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Notes)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.StockStatusName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Notes)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.StockStatusID }) |
            @Html.ActionLink("Details", "Details", new { id=item.StockStatusID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.StockStatusID })
        </td>
    </tr>
}

</table>
